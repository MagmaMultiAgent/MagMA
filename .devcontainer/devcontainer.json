{
    "name": "Thesis Development",
    "build": {
        "dockerfile": "../Dockerfile.CPU"
    },
    "customizations": {
        "vscode": {
            "extensions": ["ms-python.python", "ms-toolsai.jupyter"]
        }
    },
    "workspaceFolder": "/usr/src/luxai",
    "mounts": [
        "source=${localEnv:PWD},target=/usr/src/luxai,type=bind,consistency=cached"
    ],
    "forwardPorts": [ 8888 ]
}